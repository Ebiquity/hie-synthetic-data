                        owl:onDataRange <https://knacc.umbc.edu/dae-young/kim/ontologies/synthea#snomed:SNOMED-CT> ;
                        owl:onProperty syn:procedureCode ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] [ a owl:Restriction ;
                        owl:onDataRange <https://knacc.umbc.edu/dae-young/kim/ontologies/synthea#dicom:UID> ;
                        owl:onProperty syn:seriesUid ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] [ a owl:Restriction ;
                        owl:onDataRange <https://knacc.umbc.edu/dae-young/kim/ontologies/synthea#dicom:DICOM-SOP> ;
                        owl:onProperty syn:sopCode ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] ) ],
        syn:Synthea .

syn:Provider a owl:Class ;
    rdfs:comment "Clinicians that provide patient care." ;
    rdfs:subClassOf syn:Synthea .

syn:Payer a owl:Class ;
    rdfs:comment "Payer organization data." ;
    rdfs:subClassOf syn:Synthea .

syn:Encounter a owl:Class ;
    rdfs:comment "Patient encounter data." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange <https://knacc.umbc.edu/dae-young/kim/ontologies/synthea#snomed:SNOMED-CT> ;
            owl:onProperty syn:code ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        syn:Synthea .

syn:Patient a owl:Class ;
    rdfs:comment "Patient demographic data." ;
    rdfs:subClassOf syn:Synthea .

[] a owl:AllDisjointClasses ;
    owl:members ( syn:Allergy syn:CarePlan syn:Claim syn:ClaimTransaction syn:Condition syn:Device syn:Encounter syn:ImagingStudy syn:Immunization syn:Medication syn:Observation syn:Organization syn:Patient syn:Payer syn:PayerTransition syn:Procedure syn:Provider syn:Supply ) .

